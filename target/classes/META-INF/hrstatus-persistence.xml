<persistence xmlns="http://java.sun.com/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence
   http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd"
	version="1.0">
	<persistence-unit name="pu-hr" transaction-type="JTA">
		<provider>org.hibernate.ejb.HibernatePersistence</provider>
		<jta-data-source>java:/hrStatusDS</jta-data-source>
		<class>br.com.ohsnap.hrstatus.model.Servidores</class>
		<class>br.com.ohsnap.hrstatus.model.Users</class>
		<class>br.com.ohsnap.hrstatus.model.Configurations</class>
		<class>br.com.ohsnap.hrstatus.model.PassExpire</class>
		<exclude-unlisted-classes />
	</persistence-unit>
</persistence>